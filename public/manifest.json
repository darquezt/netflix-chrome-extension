{
  "manifest_version": 2,
  "name": "Netflix Hidden Categories",
  "description": "This extension allows you to navigate through netflix hidden categories.",
  "version": "1.0",
  "icons": {
    "16" : "assets/images/icon16.png",
    "36" : "assets/images/icon36.png",
    "128" : "assets/images/icon128.png"
  },
  "browser_action": {
    "default_icon": "assets/images/icon18.png",
    "default_popup": "public/index.html"
  },
  "permissions": [
    "tabs"
  ]
}
